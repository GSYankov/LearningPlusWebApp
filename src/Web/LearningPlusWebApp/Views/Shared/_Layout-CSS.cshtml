@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>@ViewData["Title"]</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Favicons -->
    <link href="/img/lpicon.PNG" rel="icon">
    <link href="/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,700,700i|Raleway:300,400,500,700,800|Montserrat:300,400,700" rel="stylesheet">


    <!-- Libraries CSS Files -->
    <link href="/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet">

    @RenderSection("Head", false)

    <!-- Main Stylesheet File -->
    <link href="/css/site.css" rel="stylesheet">
    @Html.Raw(JavaScriptSnippet.FullScript)
</head>

<body>

    <!--==========================
      Top Bar
    ============================-->
    <section class="topbar">
        <div class="wrapper">
            <div class="contact-info float-left">
                <i class="fa fa-envelope-o"></i> <a href="mailto:svetleto_gb@abv.bg">svetleto_gb@abv.bg</a>
                <i class="fa fa-phone"></i><a href="tel:+359883483509">0883 48-35-09</a>
            </div>
            <div class="social-links float-right">
                <a href="#" class="twitter"><i class="fa fa-twitter"></i></a>
                <a href="#" class="facebook"><i class="fa fa-facebook"></i></a>
                <a href="#" class="instagram"><i class="fa fa-instagram"></i></a>
                <a href="#" class="google-plus"><i class="fa fa-google-plus"></i></a>
                <a href="#" class="linkedin"><i class="fa fa-linkedin"></i></a>
            </div>
        </div>
    </section>

    <partial name="_HeaderPartial" />

    @RenderBody()
    <!--==========================
      Footer
    ============================-->
    <footer class="footer">
        <div class="container">
            <div class="copyright">
                &copy; Made <strong>for learning purposes with pure HTML and CSS</strong>.<br />All Rights Reserved
            </div>
        </div>
    </footer><!-- #footer -->

    <a href="#" class="back-to-top"><i class="fa fa-chevron-up"></i></a>

    <script src="~/js/loading.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
